{
  "name": "cobot",
  "version": 2,
  "alias": "cobot",
  "github": {
    "silent": true
  },
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": ".*",
      "dest": "./dist/index.html"
    }
  ]
}
